{
  "name": "PartnerToken",
  "version": "1",
  "manifest_version": 2,
  "description": "Pulls your partner center token",
  "icons": { "128" : "icon.png"},
  "homepage_url": "http://github.com/DantesDungeon",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "PartnerTokenToken"
  },
  "permissions": [
    "http://*.microsoft.com/*",
    "tabs",
	"cookies",
    "https://*.microsoft.com/*"
  ]
}